{
  "name": "@tech_query/node-toolkit",
  "version": "0.8.2",
  "description": "Toolkit for Node.JS packages",
  "keywords": [
    "toolkit",
    "toolbox",
    "utility",
    "node",
    "package",
    "library",
    "module"
  ],
  "author": "shiy2008@gmail.com",
  "license": "LGPL-3.0",
  "homepage": "https://techquery.github.io/node-toolkit/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TechQuery/node-toolkit.git"
  },
  "bugs": {
    "url": "https://github.com/TechQuery/node-toolkit/issues"
  },
  "main": "dist/index.js",
  "module": "source/index.js",
  "engines": {
    "node": "^5.12.0"
  },
  "scripts": {
    "lint": "eslint source/ test/ --fix",
    "test": "mocha --exit",
    "debug": "mocha --inspect-brk",
    "compile": "babel source/ -d dist/",
    "build": "npm run lint  &&  npm test  &&  npm run compile  &&  esdoc",
    "prepublishOnly": "npm run build",
    "start": "opn docs/index.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build  &&  git add ."
    }
  },
  "dependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "file-type": "^10.5.0",
    "fs-extra": "^7.0.1",
    "mime": "^2.4.0",
    "prettier": "^1.15.3",
    "uglify-js": "^3.4.9"
  },
  "peerDependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.1.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-external-nodejs-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.9.0",
    "husky": "^1.2.0",
    "mocha": "^5.2.0",
    "opn-cli": "^3.1.0",
    "should": "^13.2.3",
    "should-http": "^0.1.1",
    "should-sinon": "0.0.6",
    "sinon": "^7.1.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "dynamic-import-node"
    ]
  },
  "test": {
    "env": {
      "development": {
        "example": {
          "^test$": "example"
        }
      }
    }
  }
}
